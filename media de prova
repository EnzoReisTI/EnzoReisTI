    printf("digite a nota da prova:\n");
    scanf("%f", &prova);
    printf("digite a nota do simu:\n");
    scanf("%f", &simu);
    
    float media = (teste + prova + simu)/3;
    
    if(media >= 6)
    printf("Parabens voce passou");
     else{printf("infelizmente voce n√£o passou. Por favor tentar novamente");}    
